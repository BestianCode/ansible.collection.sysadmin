#!/bin/bash
#
# Network configuration script
# Generated by Ansible

# This script handles network-related startup tasks such as:
# - SSH tunnels
# - Custom routes
# - Network interface configuration
#

{% if network_startup_delay is defined and network_startup_delay > 0 %}
sleep {{ network_startup_delay }}
{% endif %}

{% if network_sh_commands is defined and network_sh_commands | length > 0 %}
### Execute network commands
{% for cmd in network_sh_commands %}
{{ cmd }}
{% endfor %}
{% endif %}
